.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .floor_group .floor_title {
  padding: 10rpx 0;
  width: 100%;
}
.index_floor .floor_group .floor_list {
  overflow: hidden;
}
.index_floor .floor_group .floor_list navigator {
  float: left;
  width: 33.3%;
  /*后四张图片链接*/
}
.index_floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  /*原图宽高  232*386
            232/386=33.33vw /height
            */
  height: 27.72711207vw;
  border-left: 10rpx solid #ffffff;
  border-bottom: 10rpx solid #ffffff;
}
.index_floor .floor_group .floor_list image {
  width: 100%;
  height: 100%;
}
